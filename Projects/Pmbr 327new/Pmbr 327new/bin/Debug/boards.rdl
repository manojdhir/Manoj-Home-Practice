  <DataSets>
    <DataSet Name="licenseverifications">
      <Query>
        <DataSourceName>licenseverifications</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@providerpicklist">
            <Value>=Parameters!providerpicklist.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@facilitypicklist">
            <Value>=Parameters!facilitypicklist.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT	Distinct     ufj.docid, 
dbb.lernam, 
dbb.lernum,
dbb.lertyp,
Case when LERMOC ='NO' then 'Not Participating in MOC'
								when LERMOC = 'Yes' then 'Participating in MOC'
						 		When LERMOC = 'Not Required' then 'Not Required to Participate in MOC'
						 End LERMOC,

             
			 COALESCE(dbb.RecertDate, dbb.CertDate) as Issued,
             COALESCE(dbb.RecertExpDate, dbb.CertExpDate) as Expires

FROM	userfacjoin ufj
                        JOIN uc_facility f ON ufj.facilityid = f.facilityid
			JOIN docdb_personal dbp ON ufj.docid = dbp.docid
                        JOIN docdb_providerfacilityspecificdata pfsd ON dbp.docid = pfsd.docid 
                                  and pfsd.facilityid = ufj.facilityid

                        JOIN DocDB_boardx dbb on dbb.docid = ufj.docid
                                  and dbb.Active_Flag =1

where ufj.facilityid in (@facilitypicklist) and ufj.docid in (@providerpicklist)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="lernam">
          <DataField>lernam</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="docid">
          <DataField>docid</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="lernum">
          <DataField>lernum</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="lertyp">
          <DataField>lertyp</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Issued">
          <DataField>Issued</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="LERMOC">
          <DataField>LERMOC</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Expires">
          <DataField>Expires</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
