    <DataSource Name="licenseverifications">
      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>Data Source=khan;Initial Catalog=UCENT</ConnectString>
      </ConnectionProperties>
      <rd:SecurityType>DataBase</rd:SecurityType>
      <rd:DataSourceID>df2c9669-9486-4a51-a5a4-3c53e054aeff</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="licenseverifications">
      <Query>
        <DataSourceName>licenseverifications</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@providerpicklist">
            <Value>=Parameters!providerpicklist.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@facilitypicklist">
            <Value>=Parameters!facilitypicklist.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT	 ufj.docid, dbc.type,dbc.certno, dbc.issuedate, 
			dbc.expdate, dbc.state


FROM	userfacjoin ufj
                        JOIN uc_facility f ON ufj.facilityid = f.facilityid
			JOIN docdb_personal dbp ON ufj.docid = dbp.docid
                        JOIN docdb_providerfacilityspecificdata pfsd ON dbp.docid = pfsd.docid 
                                  and pfsd.facilityid = ufj.facilityid

                        JOIN docdb_dea dbc ON dbc.docid = ufj.docid
                                   and (dbc.active_flag = 1 or dbc.active_flag is null)
                                   --and dbc.type = 'DEA' 

where ufj.facilityid in (@facilitypicklist) and ufj.docid in (@providerpicklist)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="type">
          <DataField>type</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="docid">
          <DataField>docid</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="certno">
          <DataField>certno</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="issuedate">
          <DataField>issuedate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="expdate">
          <DataField>expdate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="state">
          <DataField>state</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
