      </ConnectionProperties>
      <rd:SecurityType>DataBase</rd:SecurityType>
      <rd:DataSourceID>f9acd236-a84e-4013-a446-a0bc72f94d25</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="ProviderVerificaitonReport">
      <Query>
        <DataSourceName>ProviderVerificaitonReport</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@facilitypicklist">
            <Value>=Parameters!facilitypicklist.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@providerpicklist">
            <Value>=Parameters!providerpicklist.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@processstatus">
            <Value>=Parameters!processstatus.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@providertype">
            <Value>=Parameters!providertype.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT	ufj.facilityid, f.facname, dbp.docid, dbp.pslnam, dbp.psfnam, dbp.psmi, dbp.psdegree



FROM	userfacjoin ufj
                        JOIN uc_facility f ON ufj.facilityid = f.facilityid
			JOIN docdb_personal dbp ON ufj.docid = dbp.docid
                        JOIN docdb_providerfacilityspecificdata pfsd ON dbp.docid = pfsd.docid 
                                  and pfsd.facilityid = ufj.facilityid
                   LEFT JOIN docdb_professx dbprof ON dbp.docid = dbprof.docid 
                                  and dbprof.PRACID = pfsd.pracid
                                  and dbprof.PRACSUBID = pfsd.PracSubID
			
where ufj.facilityid in(@facilitypicklist)and pfsd.process_status in (@processstatus) and dbp.psprvdrtype in (@providertype)
 and ufj.docid in (@providerpicklist)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="facilityid">
          <DataField>facilityid</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="facname">
          <DataField>facname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="docid">
          <DataField>docid</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="pslnam">
          <DataField>pslnam</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="psfnam">
          <DataField>psfnam</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="psmi">
          <DataField>psmi</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="psdegree">
          <DataField>psdegree</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="facilitypicklist">
      <Query>
        <DataSourceName>ProviderVerificaitonReport</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@docid">
            <Value>=Parameters!docid.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select ufj.facilityid, ucfacility.facname

from userfacjoin ufj, uc_facility  ucfacility
where ufj.docid = @docid and ufj.facilityid = ucfacility.facilityid
order by facname asc
</CommandText>
      </Query>
      <Fields>
        <Field Name="facilityid">
          <DataField>facilityid</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="facname">
          <DataField>facname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="processstatus">
      <Query>
        <DataSourceName>ProviderVerificaitonReport</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@facilitypicklist">
            <Value>=Parameters!facilitypicklist.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select pfsd.process_status
from userfacjoin ufj
     join docdb_providerfacilityspecificdata pfsd on pfsd.facilityid = ufj.facilityid 
             and pfsd.docid = ufj.docid
             and pfsd.process_status &lt;&gt; 'Expunged'

where ufj.facilityid in (@facilitypicklist)

group by pfsd.process_status
order by pfsd.process_status</CommandText>
      </Query>
      <Fields>
        <Field Name="process_status">
          <DataField>process_status</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="providertype">
      <Query>
        <DataSourceName>ProviderVerificaitonReport</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@facilitypicklist">
            <Value>=Parameters!facilitypicklist.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@processstatus">
            <Value>=Parameters!processstatus.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select distinct dbp.psprvdrtype
from userfacjoin ufj
     join docdb_providerfacilityspecificdata pfsd on pfsd.facilityid = ufj.facilityid 
             and pfsd.docid = ufj.docid
             and pfsd.process_status &lt;&gt; 'Expunged'
     join docdb_personal dbp on dbp.docid = ufj.docid

where ufj.facilityid in (@facilitypicklist) and pfsd.process_status in(@processstatus)

order by dbp.psprvdrtype</CommandText>
      </Query>
      <Fields>
        <Field Name="psprvdrtype">
          <DataField>psprvdrtype</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="providerpicklist">
      <Query>
        <DataSourceName>ProviderVerificaitonReport</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@facilitypicklist">
            <Value>=Parameters!facilitypicklist.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@processstatus">
            <Value>=Parameters!processstatus.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@providertype">
            <Value>=Parameters!providertype.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select dbp.docid, dbp.pslnam, dbp.psfnam, dbp.psmi, dbp.psdegree
from userfacjoin ufj
            join docdb_personal dbp on dbp.docid = ufj.docid
            join docdb_providerfacilityspecificdata pfsd on pfsd.facilityid = ufj.facilityid 
                   and pfsd.docid = ufj.docid
                   and pfsd.process_status &lt;&gt; 'Expunged'
where ufj.facilityid in (@facilitypicklist) and pfsd.process_status in (@processstatus) and dbp.psprvdrtype in (@providertype)

group by dbp.docid, dbp.pslnam, dbp.psfnam, dbp.psmi, dbp.psdegree
order by dbp.pslnam asc, dbp.psfnam asc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="docid">
          <DataField>docid</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="pslnam">
          <DataField>pslnam</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="psfnam">
          <DataField>psfnam</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="psmi">
          <DataField>psmi</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="psdegree">
          <DataField>psdegree</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="providerfullname">
          <Value>=Iif(IsNothing(Fields!psmi.Value), Fields!pslnam.Value + ", " + Fields!psfnam.Value + " " + Fields!psdegree.Value,
Iif(Fields!psmi.Value = "", Fields!pslnam.Value + ", " + Fields!psfnam.Value + " " + Fields!psdegree.Value,
Fields!pslnam.Value + ", " + Fields!psfnam.Value + " " + Fields!psmi.Value + " " + Fields!psdegree.Value))</Value>
        </Field>
      </Fields>
    </DataSet>
