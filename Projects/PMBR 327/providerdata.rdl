  <DataSets>
    <DataSet Name="res1verification">
      <Query>
        <DataSourceName>providerdata</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@facilitypicklist">
            <Value>=Parameters!facilitypicklist.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@providerpicklist">
            <Value>=Parameters!providerpicklist.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT	ufj.facilityid, f.facname, dbp.docid, dbp.pslnam, dbp.psfnam, dbp.psmi, dbp.psdegree,pfsd.privstatus, pfsd.PrivLastReAppDate, pfsd.PrivReappDate, dbprof.pfnam, dbprof.pfadd1, dbprof.pfadd2,dbprof.pfcity, dbprof.pfst, dbprof.pfzip, dbprof.pfphonenbr as primarypracticePhone, dbprof.pffaxnbr as primarypracticeFax, pfsd.SPEC1, pfsd.DEPT1,(dbp.pslnam + ', ' + dbp.psfnam + Coalesce(' ' + Left(dbp.PSMI,1),'') + ' ' + Coalesce(dbp.PSSUFX,'')) as name

FROM	userfacjoin ufj
                        JOIN uc_facility f ON ufj.facilityid = f.facilityid
			JOIN docdb_personal dbp ON ufj.docid = dbp.docid
                        JOIN docdb_providerfacilityspecificdata pfsd ON dbp.docid = pfsd.docid 
                                  and pfsd.facilityid = ufj.facilityid
                   LEFT JOIN docdb_professx dbprof ON dbp.docid = dbprof.docid 
                                  and dbprof.PRACID = pfsd.pracid
                                  and dbprof.PRACSUBID = pfsd.PracSubID
			
where  ufj.facilityid in (@facilitypicklist) and ufj.docid in(@providerpicklist,',')</CommandText>
        <Timeout>6</Timeout>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="facilityid">
          <DataField>facilityid</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="providerfullname">
          <Value>=Iif(IsNothing(Fields!psmi.Value), Fields!psfnam.Value + ", " + Fields!pslnam.Value + " " + Fields!psdegree.Value,
Iif(Fields!psmi.Value = "", Fields!psfnam.Value + ", " + Fields!pslnam.Value + " " + Fields!psdegree.Value,
Fields!psfnam.Value + ", " + Fields!pslnam.Value + " " + Fields!psmi.Value + " " + Fields!psdegree.Value))</Value>
        </Field>
        <Field Name="facname">
          <DataField>facname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="docid">
