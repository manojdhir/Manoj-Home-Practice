          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DirectorID">
          <DataField>DirectorID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
