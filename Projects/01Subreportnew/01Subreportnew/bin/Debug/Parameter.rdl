  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        tblFilm.FilmID, tblFilm.FilmName, tblFilm.FilmReleaseDate, tblFilm.FilmRunTimeMinutes, tblDirector.DirectorName, tblDirector.DirectorID, tblFilm.FilmDirectorID
FROM            tblFilm INNER JOIN
                         tblDirector ON tblFilm.FilmDirectorID = tblDirector.DirectorID</CommandText>
      </Query>
      <Fields>
        <Field Name="FilmID">
          <DataField>FilmID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
