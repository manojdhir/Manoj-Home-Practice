  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        tblDirector.DirectorName, tblFilm.FilmName, tblCountry.CountryName, tblFilm.FilmRunTimeMinutes
FROM            tblDirector INNER JOIN
                         tblFilm ON tblDirector.DirectorID = tblFilm.FilmDirectorID INNER JOIN
                         tblCountry ON tblFilm.FilmCountryID = tblCountry.CountryID</CommandText>
      </Query>
      <Fields>
        <Field Name="DirectorName">
          <DataField>DirectorName</DataField>
